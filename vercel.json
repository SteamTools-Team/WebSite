{
  "rewrites": [
    {
      "source": "/fr-FR",
      "destination": "/fr.html"
    },
    {
      "source": "/fr-FR/",
      "destination": "/fr.html"
    },
    {
      "source": "/en-EN",
      "destination": "/en.html"
    },
    {
      "source": "/en-EN/",
      "destination": "/en.html"
    }
  ],
  "redirects": [
    {
      "source": "/fr",
      "destination": "/fr-FR/",
      "permanent": true
    },
    {
      "source": "/en",
      "destination": "/en-EN/",
      "permanent": true
    }
  ]
}
